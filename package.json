{
  "name": "react-mobx-starter",
  "version": "1.0.0",
  "description": "Starter project to build react applications with mobx.",
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.dev.config.js --color --progress",
    "dev": "webpack-dev-server --config config/webpack.dev.config.js --color --progress",
    "build": "webpack --config config/webpack.prod.config.js",
