    "styled-components": "^4.1.1"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.15",
    "@types/jest": "^23.3.9",
    "@types/react": "^16.7.4",
    "@types/react-dom": "^16.0.9",
    "@types/styled-components": "^4.1.0",
    "@types/webpack-env": "^1.13.6",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "react-test-renderer": "^16.6.3",
    "ts-jest": "^23.10.4",
    "ts-loader": "^5.3.0",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6",
